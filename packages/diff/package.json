    "ajv": "8.17.1",
    "cli-truncate": "5.1.0",
    "colord": "2.9.3",
    "handlebars": "4.7.8",
    "is-unicode-supported": "2.1.0",
    "strip-ansi": "7.1.2",
    "supports-hyperlinks": "4.3.0",
    "wrap-ansi": "9.0.2",
    "yaml": "2.8.1"
  },
  "scripts": {
    "build": "nx build diff",
    "lint": "nx lint diff",
    "test": "nx test diff"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
