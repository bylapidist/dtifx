{
  "name": "@dtifx/diff",
  "version": "1.0.0",
  "description": "Production DTIF diff engine with reporters, formatters, and CI integrations.",
  "license": "MIT",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/bylapidist/dtifx.git",
    "directory": "packages/diff"
  },
  "homepage": "https://dtifx.lapidist.net/diff",
  "bugs": {
    "url": "https://github.com/bylapidist/dtifx/issues"
  },
  "keywords": [
    "dtif",
    "design tokens",
    "diff",
    "ci",
    "regression"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
